<!DOCTYPE html>
<html lang="en">
    <%- include ../layouts/head.ejs %>
    <body>
        <%- include ../layouts/header.ejs %>
        <main>
            <h1>Clinics</h1>
            <% clinics.forEach(clinic => { %>
                <ul>
                    <li><a href="/clinics/show/<%= clinic.id %>"><%= clinic.name %></a></li>
                </ul>
            <% }); %>
        </main>
        <%- include ../layouts/footer.ejs %>
    </body>
</html>