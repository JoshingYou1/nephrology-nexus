<!DOCTYPE html>
<html lang="en">
    <%- include ../layouts/head.ejs %>
    <body>
        <%- include ../layouts/header.ejs %>
        <nav>
            <a href="/clinics">Clinics</a> &gt
            <a href="/clinics/<%= clinicId %>"><%= clinic.name %></a> &gt
            <a href="/clinics/<%= clinicId %>/patients">Patients</a> &gt
            <a href="/clinics/<%= clinicId %>/patients/<%= patientId %>"><%= patient.patientName %></a> &gt
            <span>Lab Results</span>
        </nav>
        <main>
            <h1>Lab Results</h1>
            <ul>
                <% results.forEach(result => { %>
                <li><a href="/clinics/<%= clinicId %>/patients/<%= patientId %>/lab-results/show/<%= result.id %>"><%= result.formatDate %></a></li>
                <% }); %>
            </ul>
        </main>
        <%- include ../layouts/footer.ejs %>
    </body>
</html>