<!DOCTYPE html>
<html lang="en">
    <%- include ../layouts/head.ejs %>
    <body>
        <%- include ../layouts/header.ejs %>
        <main>
            <h1>Lab Results</h1>
            <ul>
                <% results.forEach(result => { %>
                <ol><a href="/clinics/<%= result.patient.clinic._id %>/patients/<%= result.patient.id %>/lab-results/show/<%= result.id %>"><%= result.date %></a>
                    <a href="/clinics/<%= result.patient.clinic._id %>/patients/<%= result.patient.id %>/lab-results/update/<%= result.id %>">update</a></ol>
                <% }); %>
            </ul>
        </main>
        <%- include ../layouts/footer.ejs %>
    </body>
</html>